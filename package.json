{
	"name": "portfolio-sora",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/classnames": "^2.2.9",
		"@types/jest": "24.0.15",
		"@types/node": "12.6.8",
		"@types/react": "16.8.23",
		"@types/react-dom": "16.8.4",
		"@types/react-router-dom": "^4.3.5",
		"classnames": "^2.2.6",
		"eslint": "^6.6.0",
		"eslint-utils": "^1.4.2",
		"global": "^4.4.0",
		"lodash": "^4.17.15",
		"lodash.template": "^4.5.0",
		"node-sass": "^4.12.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.1.0",
		"react-scripts": "3.0.1",
		"typescript": "3.5.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"tslint": "yarn eslint . --cache --cache-location node_modules/.cache/eslint --ext .js,.jsx,.ts,.tsx",
		"tslint:fix": "yarn eslint . --fix --cache --cache-location node_modules/.cache/eslint --ext .js,.jsx,.ts,.tsx",
		"prettier": "prettier --check prettier.config.json \"./src/**/*.{js,ts,tsx,jsx,json,md,scss}\"",
		"prettier:fix": "prettier --write prettier.config.json \"./src/**/*.{js,ts,tsx,jsx,json,md,scss}\"",
		"precommit": "yarn prettier && yarn tslint",
		"precommit:fix": "yarn tslint:fix && yarn prettier:fix"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"eslint-config-prettier": "^6.0.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.16.0",
		"prettier": "^1.18.2"
	}
}
